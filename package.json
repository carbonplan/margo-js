{
  "name": "margo-js",
  "version": "1.0.0",
  "description": "pure javascript implementation of the margo climate model",
  "main": "dst/margo-js.cjs.js",
  "module": "dst/margo-js.esm.js",
  "browser": "dst/margo-js.umd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "build-demo": "(rollup src/index.js --file demo/lib/margo-js.js --format cjs) && (cd demo && npm run build)",
    "dev-demo": "(cd demo && npm run dev) & rollup src/index.js --file demo/lib/margo-js.js --format cjs -w",
    "start-demo": "npm run build-demo && (cd demo && npm run start)",
    "install-demo": "cd demo && npm install",
    "test": "node test/test.js | tap-spec",
    "pretest": "npm run build",
    "format-js": "prettier --write 'src/**/*.js' 'test/*.js' 'demo/{components,pages,lib}/**/*.js'",
    "format-md": "prettier --write 'demo/demo.md' 'README.md' --parser mdx",
    "format": "npm run format-js && npm run format-md"
  },
  "files": [
    "dst"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbonplan/margo-js.git"
  },
  "keywords": [
    "climate",
    "simulation",
    "model",
    "margo",
    "physics"
  ],
  "author": "carbonplan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carbonplan/margo-js/issues"
  },
  "homepage": "https://github.com/carbonplan/margo-js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "prettier": "^2.1.1",
    "rollup": "^2.26.8",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  }
}
